<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.me.guanpj.mall.user">

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name="com.me.guanpj.mall.library.core.BaseApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".module.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

		<activity android:name=".module.register.RegisterActivity"/>
        <activity android:name=".module.forgetpwd.ForgetPwdActivity"/>
        <activity android:name=".module.resetpwd.ResetPwdActivity"/>
        <activity android:name=".module.user.UserInfoActivity"/>

    </application>

</manifest>
